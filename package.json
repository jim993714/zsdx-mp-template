    "lint:js": "eslint --ext .ts,.js,.vue src --fix",
    "lint:css": "stylelint \"src/**/*.(vue|scss|css|less)\" --fix",
    "lint": "prettier --write . && npm run lint:js && npm run lint:css",
    "prepare": "husky install && cp -i ./src/pages.base.json ./src/pages.json && node ./plugins/build.mjs"
